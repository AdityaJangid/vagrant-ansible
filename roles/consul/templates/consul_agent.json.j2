{
  "server": true,
  "node_name": "{{ consul_node_name }}",
  "datacenter": "{{ consul_datacenter }}",
  "data_dir": "{{consul_data_dir}}",
  "bind_addr": "0.0.0.0",
  "client_addr": "0.0.0.0",
  "advertise_addr": "{{ ansible_default_ipv4.address }}",
  "bootstrap_expect": 1,
  "retry_join": ["{{ ansible_default_ipv4.address }}"],
  "ui": true,
  "log_level": "DEBUG",
  "enable_syslog": true,
  "acl_enforce_version_8": false
}